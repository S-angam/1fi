      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sangam.authentication"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "myapp",
    "extra": {
      "eas": {
        "projectId": "311fe254-ad53-4a75-ae66-5fe734152b2a"
      }
    }
  }
}
